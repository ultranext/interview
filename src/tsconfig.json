{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "baseUrl": "./",
      "paths": {
        "@/*": ["./src/*"],
        "@rpc/*": ["./rpc/types/*"],
        "keywordsPacket/*": ["./src/plugin/keywordsPacket/*"],
        "liteKeywordsPacket/*": ["./src/plugin/liteKeywordsPacket/*"]
      },
      "lib": ["dom", "dom.iterable", "esnext"],
      "sourceMap": false,
      "allowJs": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noEmit": false,
      "strict": true,
      "skipLibCheck": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "jsx": "react",
      "declaration": false,
      "downlevelIteration": true
    },
    "include": ["src"],
    "exclude": ["rpc"]
  }
  